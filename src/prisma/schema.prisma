generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @unique @default(uuid())
  createdIn DateTime
  updated   DateTime

  name     String
  email    String @unique
  password String

  @@map("user")
}
