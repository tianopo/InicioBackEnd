generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id           String   @id @unique @default(uuid())
  criadoEm     DateTime
  atualizadoEm DateTime

  nome  String
  email String @unique
  senha String

  @@map("user")
}
